<header>
  <nav class="navbar navbar-expand-md navbar-dark bg-success">
    <a class="navbar-brand" href="/urls">TinyApp</a>
    <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNavAltMarkup" aria-controls="navbarNavAltMarkup" aria-expanded="false" aria-label="Toggle navigation">
      <span class="navbar-toggler-icon"></span>
    </button>
    <div class="collapse navbar-collapse" id="navbarNavAltMarkup">
      <div class="navbar-nav">
        <a class="nav-item nav-link" href="/urls">My URLs</a>
        <a class="nav-item nav-link" href="/urls/new">Create New URL</a>
        <nav class="navbar navbar-dark">
              <% if (user) { %>
                <h5>Logged In As: <%= user.email %>!</h5>
                <form class="form-inline" action="/logout" method="POST">
                  <div style= "margin-left:500px"></div> 
                  <button class="btn btn-outline-dark" type="submit">Logout</button>
                </form>
              <% } else { %>
                <form class="form-inline" action="/login" method="GET">
                    <div style= "margin-left:600px"></div>
                    <button class="btn btn-outline-light" type="submit">Login</button>
                </form>
              <% } %>
              
            <form action="/register" method="GET">
              <main style="margin: 5px;">
              <button class="btn btn-outline-light">Register</button>
            </form>
        </nav>
      </div>
    </div>
  </nav>
</header>